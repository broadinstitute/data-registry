{
    "private": true,
    "scripts": {
        "build": "nuxt build",
        "dev": "set NUXT_PUBLIC_SKIP_AUTH=true && nuxt dev --host local.kpndataregistry.org",
        "dev-auth": "nuxt dev --host local.kpndataregistry.org",
        "dev-local": "set NODE_TLS_REJECT_UNAUTHORIZED=0 && set NUXT_PUBLIC_API_BASE_URL=https://api.kpndataregistry.org:8000 && nuxt dev --host local.kpndataregistry.org --port 443 --https --ssl-cert ../certificate.crt --ssl-key ../private.key",
        "test": "vitest --run",
        "lint": "eslint --ext .js,.vue --ignore-path .gitignore .",
        "lintfix": "eslint --fix --ext .js,.vue --ignore-path .gitignore .",
        "cy:open": "cypress open",
        "test:build": "nuxt build && nuxt start",
        "cy:run": "cypress run",
        "test:e2e": "start-server-and-test test:build http://local.kpndataregistry.org:3000 cy:run"
    },
    "devDependencies": {
        "@nuxt/devtools": "1.3.9",
        "@nuxt/eslint-config": "0.3.13",
        "@nuxtjs/eslint-config": "12.0.0",
        "@nuxtjs/tailwindcss": "^6.12.1",
        "@primevue/nuxt-module": "^4.0.4",
        "@stylistic/eslint-plugin-js": "2.3.0",
        "@vitejs/plugin-vue": "5.1.2",
        "cypress": "^13.13.2",
        "cypress-file-upload": "^5.0.8",
        "eslint": "8.57.0",
        "eslint-config-standard": "17.1.0",
        "eslint-plugin-import": "2.29.1",
        "eslint-plugin-n": "^16.6.2",
        "eslint-plugin-promise": "6.4.0",
        "eslint-plugin-vue": "9.27.0",
        "nuxt": "3.12.4",
        "nuxt-primevue": "^3.0.0",
        "nuxt-shiki": "^0.3.0",
        "playwright-core": "1.46.0",
        "sass": "1.77.8",
        "sass-loader": "16.0.0",
        "start-server-and-test": "^2.0.5",
        "tailwindcss": "3.4.9",
        "tailwindcss-primeui": "^0.3.4",
        "vite": "5.4.0",
        "vitest": "1.6.0",
        "vue-router": "4.4.3"
    },
    "dependencies": {
        "@pinia/nuxt": "0.5.3",
        "@primevue/themes": "^4.0.4",
        "axios": "1.7.3",
        "bootstrap": "5.3.3",
        "bootstrap-icons": "1.11.3",
        "chart.js": "4.4.3",
        "d3": "7.9.0",
        "primeicons": "7.0.0",
        "primevue": "4.0.4"
    }
