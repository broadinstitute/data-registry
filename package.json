        "@vue/test-utils": "2.4.5",
        "cypress": "^13.9.0",
        "cypress-file-upload": "^5.0.8",
        "eslint": "8.57.0",
        "eslint-config-standard": "17.1.0",
        "eslint-plugin-import": "2.29.1",
        "eslint-plugin-n": "^16.6.2",
        "eslint-plugin-promise": "6.1.1",
        "eslint-plugin-vue": "9.25.0",
        "nuxt": "3.11.2",
        "nuxt-primevue": "^0.3.1",
        "nuxt-shiki": "^0.3.0",
        "playwright-core": "1.43.1",
