{
    "private": true,
    "scripts": {
        "build": "nuxt build",
        "dev": "set NUXT_PUBLIC_SKIP_AUTH=true && nuxt dev --host local.kpndataregistry.org",
        "dev-auth": "nuxt dev --host local.kpndataregistry.org",
        "dev-local": "set NODE_TLS_REJECT_UNAUTHORIZED=0 && set NUXT_PUBLIC_API_BASE_URL=https://api.kpndataregistry.org:8000 && nuxt dev --host local.kpndataregistry.org --port 443 --https --ssl-cert ../certificate.crt --ssl-key ../private.key",
        "test": "vitest --run",
        "lint": "eslint --ext .js,.vue --ignore-path .gitignore .",
        "lintfix": "eslint --fix --ext .js,.vue --ignore-path .gitignore .",
        "cy:open": "cypress open",
        "cy:run": "cypress run",
        "test:e2e": "start-server-and-test dev-auth http://local.kpndataregistry.org:3000 cy:run"
    },
    "devDependencies": {
        "@nuxt/devtools": "1.2.0",
        "@nuxt/eslint-config": "0.3.13",
